{
  "name": "wysiwyg-html-editor",
  "version": "0.0.1",
  "private": true,
  "description": "A reusable \"What You See Is What You Get\" html text editor library for PureCloud",
  "main": "lib.js",
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/inindca/wysiwyg-html-editor.git"
  },
  "scripts": {
    "build": "npm run bundle && npm run doc",
    "bundle": "webpack -p --config webpack.config.js",
    "doc": "node node_modules/jsdoc-to-markdown/bin/cli.js --plugin dmd-bitbucket --src \"src/**/*.js\" > API.md",
    "example": "echo 'Open a browser to localhost:8000/example' && python -m SimpleHTTPServer",
    "pre-push": ". ./githooks/pre-push",
    "watch": "nodemon --watch src -e js,html --exec \"npm run build\""
  },
  "author": "Interactive Intelligence, Inc.",
  "homepage": "https://bitbucket.org/inindca/wysiwyg-html-editor#readme",
  "devDependencies": {
    "css-loader": "^0.23.1",
    "dmd-bitbucket": "^0.1.9",
    "eslint": "^2.10.2",
    "eventemitter3": "^1.2.0",
    "html-loader": "^0.4.3",
    "imports-loader": "^0.6.5",
    "jsdoc-to-markdown": "^1.3.6",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "pre-push": "^0.1.1",
    "quill": "^1.0.0-beta.2",
    "style-loader": "^0.13.1"
  },
  "pre-push": [
    "pre-push"
  ]
}

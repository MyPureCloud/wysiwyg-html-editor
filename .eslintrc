{
    "rules": {
        "consistent-return": 1,
        "eqeqeq": 2,
        "indent": [
            2,
            4,
            {"SwitchCase": 1}
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "no-alert": 1,
        "no-console": 1,
        "semi": [
            2,
            "always"
        ],
        "strict": 2,
        "no-empty-function": 2,
        "no-return-assign": 2,
        "no-self-compare": 2,
        "no-throw-literal": 2,
        "no-void": 2,
        "no-with": 2,
        "no-unused-vars": [
            2,
            {
                "args": "all",
                #Unused args should begin and end with an underscore
                "argsIgnorePattern": "(^.+_$)",
            }
        ]
    },
    "env": {
        "commonjs": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}
